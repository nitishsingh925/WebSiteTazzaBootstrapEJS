<header id="full_nav">
  <div class="header">
    <div class="container">
      <nav class="navbar navbar-expand-lg">
        <a class="navbar-brand" href="/">
          <img src="images/logo.png" alt="logo"
        /></a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarNav"
          aria-controls="navbarNav"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <!-- <span class="navbar-toggler-icon"></span> -->
          <i class="fas fa-stream navbar-toggler-icon"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav mx-auto">
            <li class="nav-item">
              <a
                class="nav-link <% if (requestUrl.url == '/'){ %>active<%}%>"
                aria-current="page"
                href="/"
                >Home</a
              >
            </li>
            <li class="nav-item">
              <a
                class="nav-link <% if (requestUrl.url == '/about'){ %>active<%}%>"
                href="about"
                >about</a
              >
            </li>

            <li class="nav-item">
              <a
                class="nav-link <% if (requestUrl.url == '/product'){ %>active<%}%>"
                href="product"
                >product</a
              >
            </li>
            <li class="nav-item">
              <a
                class="nav-link <% if (requestUrl.url == '/gallery'){ %>active<%}%>"
                href="gallery"
                >gallery</a
              >
            </li>
            <li class="nav-item">
              <a
                class="nav-link <% if (requestUrl.url == '/contact'){ %>active<%}%>"
                href="contact"
                >contact</a
              >
            </li>
          </ul>
          <div class="header_right">
            <div class="text-lg-end">
              <span>Call Us!</span>
              <span class="phone_no">+91 9876543210</span>
            </div>
          </div>
        </div>
      </nav>
    </div>
  </div>
</header>
